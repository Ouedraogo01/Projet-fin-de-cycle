@article{gagnon_presente_nodate,
	title = {présenté par : {Isabelle} {ROY}},
	abstract = {This master’s thesis contributes to the identification of factors linked to the occurrence of diarrhoea among children under five years of age in Haiti. Following a review of the literature on predictors of childhood diarrhoea in low-income countries, namely Haiti, we take advantage of four databases from Demographic and Health Surveys (n = 14 481) to conduct our quantitative analyses. Our study is concerned mainly with the impact of improved drinking water, improved sanitations and refrigerator on childhood diarrhoea. We use logistic regression models to assess the robustness of the associations between selected factors and the occurrence of diarrhoea among children under five years of age.},
	language = {fr},
	author = {Gagnon, Alain and Defo, Barthelemy KUATE and Legrand, Thomas},
	file = {PDF:files/108/Gagnon et al. - présenté par  Isabelle ROY.pdf:application/pdf},
}

